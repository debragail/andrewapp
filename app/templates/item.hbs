{{#each model as |item|}}
  <article class="item">
    <h3>{{item.id}}</h3>
    <div class="detail who">
      <span>Who:</span> {{item.who}}
    </div>
    <div class="detail what">
      <span>What:</span> {{item.what}}
    </div>
    <div class="detail when">
      <span>When:</span> {{item.when}}
    </div>
    <div class="detail desc">
      <span>Desc:</span> {{item.desc}}
    </div>
  </article>
{{/each}}
{{outlet}}
